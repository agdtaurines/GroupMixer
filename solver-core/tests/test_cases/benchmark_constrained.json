      "stop_conditions": { "max_iterations": 10000 },
      "solver_params": {
        "solver_type": "SimulatedAnnealing",
        "initial_temperature": 10.0,
        "final_temperature": 0.001,
        "cooling_schedule": "geometric",
        "log_frequency": 2000
      }
    }
  },
  "expected": {},
  "test_settings": {
    "log_test_name": true,
    "log_duration_and_score": true,
