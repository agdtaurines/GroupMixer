{
  "name": "Score Breakdown Demo",
  "input": {
    "problem": {
      "people": [
        { "id": "p0", "attributes": {} },
        { "id": "p1", "attributes": {} },
        { "id": "p2", "attributes": {} },
        { "id": "p3", "attributes": {} }
      ],
      "groups": [
        { "id": "g0", "size": 2 },
        { "id": "g1", "size": 2 }
      ],
      "num_sessions": 1
    },
    "objectives": [
      {
        "type": "maximize_unique_contacts",
        "weight": 1.0
      }
    ],
    "constraints": [
      {
        "type": "MustStayTogether",
        "people": ["p0", "p1"],
        "penalty_weight": 1000.0
      },
      {
        "type": "MustStayTogether", 
        "people": ["p2", "p3"],
        "penalty_weight": 5.0
      },
      {
        "type": "CannotBeTogether",
        "people": ["p0", "p2"],
        "penalty_weight": 10.0
      }
    ],
    "solver": {
      "solver_type": "SimulatedAnnealing",
      "stop_conditions": {
        "max_iterations": 100
      },
      "solver_params": {
        "solver_type": "SimulatedAnnealing",
        "initial_temperature": 10.0,
        "final_temperature": 0.1,
        "cooling_schedule": "geometric"
      },
      "logging": {
        "log_frequency": 0,
        "log_duration_and_score": true,
        "display_final_schedule": false,
        "log_initial_score_breakdown": true,
        "log_final_score_breakdown": true,
        "log_stop_condition": false,
        "log_initial_state": false
      }
    }
  },
  "expected": {}
} 